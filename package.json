{
  "name": "google-books",
  "version": "1.0.0",
  "description": "react google books search",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rbartl2/google-books.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rbartl2/google-books/issues"
  },
  "homepage": "https://github.com/rbartl2/google-books#readme",
  "dependencies": {
    "axios": "^0.19.1",
    "body-parser": "^1.20.1",
    "express": "^4.18.2",
    "if-env": "^1.0.4",
    "mongoose": "^5.8.6",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "concurrently": "^5.0.2",
    "nodemon": "^2.0.2"
  }
}
